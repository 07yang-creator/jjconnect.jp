<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.jjconnect.backup</string>
  <key>ProgramArguments</key>
  <array>
    <string>/bin/bash</string>
    <string>-c</string>
    <string>cd REPO_ROOT_PLACEHOLDER &amp;&amp; ./scripts/backup-run.sh</string>
  </array>
  <key>StartCalendarInterval</key>
  <dict>
    <key>Hour</key>
    <integer>2</integer>
    <key>Minute</key>
    <integer>0</integer>
  </dict>
  <key>StandardOutPath</key>
  <string>REPO_ROOT_PLACEHOLDER/scripts/backup.log</string>
  <key>StandardErrorPath</key>
  <string>REPO_ROOT_PLACEHOLDER/scripts/backup.err</string>
</dict>
</plist>
